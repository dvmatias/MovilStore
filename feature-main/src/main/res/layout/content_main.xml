<?xml version="1.0" encoding="utf-8"?><androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"	xmlns:app="http://schemas.android.com/apk/res-auto"	xmlns:tools="http://schemas.android.com/tools"	android:id="@+id/nestedScrollView"	android:layout_width="match_parent"	android:layout_height="wrap_content"	tools:context="com.matias.features.ui.main.activity.MainActivity">	<androidx.constraintlayout.widget.ConstraintLayout		android:id="@+id/containerMain"		android:layout_width="match_parent"		android:layout_height="wrap_content"		android:nestedScrollingEnabled="true">		<androidx.constraintlayout.widget.Guideline			android:id="@+id/guidelineStart"			android:layout_width="match_parent"			android:layout_height="wrap_content"			android:orientation="vertical"			app:layout_constraintGuide_begin="0dp" />		<androidx.constraintlayout.widget.Guideline			android:id="@+id/guidelineEnd"			android:layout_width="wrap_content"			android:layout_height="wrap_content"			android:orientation="vertical"			app:layout_constraintGuide_end="0dp" />		<!-- NOVELTY SECTION -->		<com.matias.components.viewpager.StylingWrapContentHeightViewPager			android:id="@+id/pagerNovelty"			android:layout_width="0dp"			android:layout_height="wrap_content"			android:background="@color/colorWhite"			android:tag="animate_in"			app:layout_constraintEnd_toEndOf="parent"			app:layout_constraintStart_toStartOf="parent"			app:layout_constraintTop_toTopOf="parent" />		<!-- ANNOUNCEMENT SECTION -->		<include			android:id="@+id/sectionAnnouncement"			layout="@layout/section_announcement"			android:layout_width="match_parent"			android:layout_height="wrap_content"			android:tag="animate_in"			app:layout_constraintEnd_toEndOf="parent"			app:layout_constraintStart_toStartOf="parent"			app:layout_constraintTop_toBottomOf="@id/pagerNovelty" />		<!-- NEW PRODUCT SECTION -->		<androidx.constraintlayout.widget.ConstraintLayout			android:id="@+id/sectionNew"			android:layout_width="match_parent"			android:layout_height="wrap_content"			android:layout_marginTop="@dimen/dimen_section_margin_top"			android:background="@color/colorWhite"			android:nestedScrollingEnabled="true"			android:paddingTop="@dimen/dimen_8dp"			android:paddingBottom="@dimen/dimen_8dp"			android:tag="animate_in"			app:layout_constraintEnd_toEndOf="parent"			app:layout_constraintStart_toStartOf="parent"			app:layout_constraintTop_toBottomOf="@id/sectionAnnouncement">			<androidx.appcompat.widget.AppCompatTextView				android:id="@+id/tvNewProductTitle"				style="@style/SectionTitleText"				android:layout_width="match_parent"				android:layout_height="wrap_content"				android:layout_marginStart="@dimen/dimen_16dp"				android:layout_marginEnd="@dimen/dimen_16dp"				android:text="@string/new_product_section_title"				app:layout_constraintEnd_toEndOf="parent"				app:layout_constraintStart_toStartOf="parent"				app:layout_constraintTop_toTopOf="parent" />			<androidx.recyclerview.widget.RecyclerView				android:id="@+id/recyclerNewProduct"				android:layout_width="match_parent"				android:layout_height="wrap_content"				android:nestedScrollingEnabled="true"				app:layout_constraintBottom_toBottomOf="parent"				app:layout_constraintStart_toStartOf="parent"				app:layout_constraintTop_toBottomOf="@id/tvNewProductTitle" />		</androidx.constraintlayout.widget.ConstraintLayout>		<!-- OFFERS SECTION -->		<include			android:id="@+id/sectionOffers"			layout="@layout/section_offers"			android:layout_width="0dp"			android:layout_height="wrap_content"			android:layout_marginTop="@dimen/dimen_section_margin_top"			android:paddingTop="@dimen/dimen_8dp"			android:paddingBottom="@dimen/dimen_8dp"			android:tag="animate_in"			app:layout_constraintEnd_toEndOf="@id/guidelineEnd"			app:layout_constraintStart_toStartOf="@id/guidelineStart"			app:layout_constraintTop_toBottomOf="@id/sectionNew" />		<!-- SERVICES -->		<include			android:id="@+id/sectionServices"			layout="@layout/section_services"			android:layout_width="0dp"			android:layout_height="wrap_content"			android:layout_marginTop="@dimen/dimen_section_margin_top"			android:paddingTop="@dimen/dimen_8dp"			android:paddingBottom="@dimen/dimen_8dp"			android:tag="animate_in"			app:layout_constraintEnd_toEndOf="@id/guidelineEnd"			app:layout_constraintStart_toStartOf="@id/guidelineStart"			app:layout_constraintTop_toBottomOf="@id/sectionOffers" />	</androidx.constraintlayout.widget.ConstraintLayout></androidx.core.widget.NestedScrollView>